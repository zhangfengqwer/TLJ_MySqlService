<?xml version="1.0" encoding="utf-8"?>

<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="NhInterMySQL"
                   namespace="NhInterMySQL.Model">

  <class name="UserInfo" table="user_info">
    <id name="Uid" column="uid" type="String">
      <generator class="assigned"></generator>
    </id>
    <property name="NickName" column="nickname" type="String"></property>
    <property name="Phone" column="phone" type="String"></property>
    <property name="Gold" column="gold" type="Int32"></property>
    <property name="Head" column="head" type="Int32"></property>
    <property name="Medel" column="medal" type="Int32"></property>
    <property name="RechargeVip" column="recharge_vip" type="Int32"></property>
    <property name="YuanBao" column="yuanbao" type="Int32"></property>
    <property name="freeCount" column="freeCount" type="Int32"></property>
    <property name="huizhangCount" column="huizhangCount" type="Int32"></property>
    <property name="luckyValue" column="luckyValue" type="Int32"></property>
  </class>

</hibernate-mapping>