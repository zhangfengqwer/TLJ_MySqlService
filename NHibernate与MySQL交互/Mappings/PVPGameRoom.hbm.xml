<?xml version="1.0" encoding="utf-8"?>

<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="NhInterMySQL"
                   namespace="NhInterMySQL.Model">

  <class name="PVPGameRoom" table="pvpgameroom">
    <id name="id" column="id" type="Int32">
      <generator class="native"></generator>
    </id>
    <property name="gameroomtype" column="gameroomtype" type="String"></property>
    <property name="gameroomname" column="gameroomname" type="String"></property>
    <property name="reward" column="reward" type="String"></property>
    <property name="baomingfei" column="baomingfei" type="String"></property>
    <property name="kaisairenshu" column="kaisairenshu" type="Int32"></property>
  </class>

</hibernate-mapping>