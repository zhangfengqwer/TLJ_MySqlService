<?xml version="1.0" encoding="utf-8"?>

<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="Zfstu"
                   namespace="Zfstu.Model">

  <class name="User" table="user">
    <id name="UserId" column="user_id" type="Int32">
      <generator class="native"></generator>
    </id>
    <property name="Username" column="username" type="String"></property>
    <property name="Userpassword" column="userpassword" type="String"></property>
    <property name="Uid" column="uid" type="String"></property>
    <property name="ThirdId" column="third_id" type="String"></property>
    <property name="Platform" column="platform" type="Int32"></property>
    <property name="IsRobot" column="is_robot" type="Int32"></property>
    <one-to-one name="USign" cascade="all" />
  </class>

</hibernate-mapping>