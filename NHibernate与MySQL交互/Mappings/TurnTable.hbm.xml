<?xml version="1.0" encoding="utf-8"?>

<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="NhInterMySQL"
                   namespace="NhInterMySQL.Model">

  <class name="TurnTable" table="turntable">
    <id name="id" column="id" type="Int32">
      <generator class="native"></generator>
    </id>
    <property name="reward" column="reward" type="String"></property>
    <property name="probability" column="probability" type="Int32"></property>
  </class>

</hibernate-mapping>