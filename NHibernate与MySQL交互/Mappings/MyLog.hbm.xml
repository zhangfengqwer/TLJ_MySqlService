<?xml version="1.0" encoding="utf-8"?>

<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="NhInterMySQL"
                   namespace="NhInterMySQL.Model">

  <class name="MyLog" table="my_log">
    <id name="log_id" column="log_id" type="Int32">
      <generator class="native"></generator>
    </id>
    <property name="uid" column="uid" type="String"></property>
    <property name="optype" column="optype" type="String"></property>
    <property name="message" column="message" type="String"></property>
  </class>
</hibernate-mapping>