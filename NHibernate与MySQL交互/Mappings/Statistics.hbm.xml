<?xml version="1.0" encoding="utf-8"?>

<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="NhInterMySQL"
                   namespace="NhInterMySQL.Model">

  <class name="Statistics" table="statistics">
    <id name="id" column="id" type="Int32">
      <generator class="native"></generator>
    </id>
    <property name="login_count" column="login_count" type="Int32"></property>
    <property name="register_count" column="register_count" type="Int32"></property>
    <property name="recharge_total" column="recharge_total" type="float"></property>
    <property name="time" column="time" type="String"></property>
    <property name="recharge_person_count" column="recharge_person_count" type="Int32"></property>
  </class>

</hibernate-mapping>