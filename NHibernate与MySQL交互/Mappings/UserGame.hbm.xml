<?xml version="1.0" encoding="utf-8"?>

<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="Zfstu"
                   namespace="Zfstu.Model">

  <class name="UserGame" table="user_game">
    <id name="Uid" column="uid" type="String">
      <generator class="assigned"></generator>
    </id>
    <property name="AllGameCount" column="all_game_count" type="Int32"></property>
    <property name="WinCount" column="win_count" type="Int32"></property>
    <property name="RunCount" column="run_count" type="Int32"></property>
    <property name="MeiliZhi" column="meilizhi" type="Int32"></property>
  </class>

</hibernate-mapping>