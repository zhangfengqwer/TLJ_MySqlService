<?xml version="1.0" encoding="utf-8"?>

<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="NhInterMySQL"
                   namespace="NhInterMySQL.Model">

  <class name="Log_Online_Player" table="log_online_player">
    <id name="id" column="id" type="Int32">
      <generator class="native"></generator>
    </id>
    <property name="Uid" column="uid" type="String"></property>
    <property name="room_id" column="room_id" type="Int32"></property>
    <property name="gameroomtype" column="gameroomtype" type="String"></property>
    <property name="ip" column="ip" type="String"></property>
    <property name="channel_name" column="channel_name" type="String"></property>
    <property name="version_name" column="version_name" type="String"></property>
    <property name="is_robot" column="is_robot" type="Int32"></property>
  </class>
</hibernate-mapping>