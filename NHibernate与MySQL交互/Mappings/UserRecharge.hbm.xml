<?xml version="1.0" encoding="utf-8"?>

<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="NhInterMySQL"
                   namespace="NhInterMySQL.Model">

  <class name="UserRecharge" table="user_recharge">
    <composite-id>
      <key-property  name="Uid" column="uid" type="String"></key-property>
      <key-property  name="goods_id" column="goods_id" type="Int32"></key-property>
    </composite-id>
    <property name="recharge_count" column="recharge_count" type="Int32"></property>
  </class>
</hibernate-mapping>