<?xml version="1.0" encoding="utf-8"?>

<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="Zfstu"
                   namespace="Zfstu.Model">

  <class name="SignConfig" table="sign_config">
    <id name="id" column="id" type="Int32">
      <generator class="native"></generator>
    </id>
    <property name="goods_name" column="goods_name" type="String"></property>
    <property name="goods_icon" column="goods_icon" type="String"></property>
    <property name="goods_prop" column="goods_prop" type="String"></property>
  </class>

</hibernate-mapping>