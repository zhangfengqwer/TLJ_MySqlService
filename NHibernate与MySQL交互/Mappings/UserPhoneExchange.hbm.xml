<?xml version="1.0" encoding="utf-8"?>

<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="NhInterMySQL"
                   namespace="NhInterMySQL.Model">

  <class name="UserPhoneExchange" table="user_phone_exchange">
    <id name="Id" column="id" type="Int32">
      <generator class="native"></generator>
    </id>
    <property name="Uid" column="uid" type="String"></property>
    <property name="Phone" column="phone" type="String"></property>
    <property name="Money" column="money" type="Int32"></property>
    <property name="CreateTime" column="create_time" type="Timestamp"></property>
  </class>

</hibernate-mapping>